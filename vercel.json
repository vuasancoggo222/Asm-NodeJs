{
    "version": 2,
    "builds": [
        {
            "src": "/src/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "/src/uploads",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/src/uploads"
        }
    ]
}
