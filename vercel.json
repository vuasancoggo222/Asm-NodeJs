{
    "version": 2,
    "builds": [
        {
            "src": "./src/app.js",
            "use": "@vercel/node"
            
        },
        {
            "src": "/src",
            "use": "@vercel/static"
        }

      
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/src/app.js"
        }
    ]
}
